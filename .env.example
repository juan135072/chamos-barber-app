# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Para generar tipos TypeScript (desarrollo):
# npx supabase gen types typescript --project-id YOUR_PROJECT_ID --schema public > lib/database.types.ts

# Configuración de producción (opcional)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OneSignal Push Notifications
# App ID: Público (puede estar en código)
NEXT_PUBLIC_ONESIGNAL_APP_ID=63aa14ec-de8c-46b3-8949-e9fd221f8d70

# Habilitar/Deshabilitar OneSignal (true por defecto)
NEXT_PUBLIC_ONESIGNAL_ENABLED=true

# REST API Key: PRIVADO (solo para backend/edge functions)
# Obtener de: https://app.onesignal.com/apps/YOUR_APP_ID/settings
ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key